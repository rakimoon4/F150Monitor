#!/bin/sh

DIRNAME=$(dirname "$0")
APP_BASE_NAME=$(basename "$0")
cd "$DIRNAME" || exit 1

warn () { echo "$*"; }
die () { echo "$*"; exit 1; }

JAVACMD=${JAVA_HOME:+$JAVA_HOME/bin/}java

CLASSPATH=$DIRNAME/gradle/wrapper/gradle-wrapper.jar

exec "$JAVACMD" -classpath "$CLASSPATH" org.gradle.wrapper.GradleWrapperMain "$@"
